{
    "name": "cardikit/cardikit",
    "description": "The number one open source digital business card maker.",
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Cardikit\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "DamoFD",
            "email": "voshall@yahoo.com"
        }
    ],
    "require-dev": {
        "pestphp/pest": "^3.8"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
